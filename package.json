{
  "name": "weekplan",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "npm run test:compile && node --experimental-specifier-resolution=node --test $(find .test-dist -name '*.test.js' -type f)",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:watch": "npm run test:compile && node --experimental-specifier-resolution=node --test --watch $(find .test-dist -name '*.test.js' -type f)"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "express": "^4.19.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.6"
  }
}
